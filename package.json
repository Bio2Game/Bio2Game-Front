    "@nuxtjs/robots": "^2.4.2",
    "chart.js": "^2.9.4",
    "chartjs-plugin-labels": "^1.1.0",
    "core-js": "^3.18.0",
    "lottie-web": "^5.7.13",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "nuxt": "^2.15.8",
    "nuxt-webfontloader": "^1.1.0",
    "simple-markdown": "^0.7.3",
    "simplemde": "^1.11.2",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.3.0",
    "string-to-vue": "^1.1.2",
